{
  "name": "ngx-photo-editor-app",
  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "host": "ng serve --host=0.0.0.0 --disable-host-check",
    "build": "ng build --base-href /ngx-photo-editor/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "npm run build && npx angular-cli-ghpages --dir=dist/ngx-photo-editor-app",
    "build:lib": "ng build ngx-photo-editor",
    "test:lib": "ng test ngx-photo-editor",
    "lint:lib": "ng lint ngx-photo-editor",
    "lint:publish": "npm publish dist/ngx-photo-editor/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.3",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@fortawesome/angular-fontawesome": "~0.11.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "ngx-photo-editor": "file:dist/ngx-photo-editor",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "angular-cli-ghpages": "^1.0.0",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.7.2"
  }
}
